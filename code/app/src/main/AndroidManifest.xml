<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.afinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name = ".StudentMainActivity"></activity>
        <activity android:name = ".FacultyMainActivity"></activity>
        <activity android:name = ".UserActivity"></activity>
        <activity android:name = ".ProjectCreate"></activity>
        <activity android:name = ".ProjectSel"></activity>
        <activity android:name = ".GroupCreate"></activity>
        <activity android:name = ".UploadActivity"></activity>
        <activity android:name = ".AddStudent"></activity>
        <activity android:name = ".DelStudent"></activity>
    </application>

</manifest>